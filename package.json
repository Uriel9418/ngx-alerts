{
  "name": "@ngx-plus/ngx-alerts",
  "version": "0.2.0",
  "description": "Advanced Angular Alerts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-plus/ngx-alerts.git"
  },
  "homepage": "https://github.com/ngx-plus/ngx-alerts#readme",
  "bugs": {
    "url": "https://github.com/ngx-plus/ngx-alerts/issues"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "alerts",
    "ngx-alerts",
    "ngx-plus",
    "colmena",
    "ngx"
  ],
  "contributors": [
    "Brannon N. Darby II <brannon@ngx.plus> (https://github.com/brannon-darby)",
    "Bram Borggreve <borggreve@gmail.com> (https://github.com/beeman)"
  ],
  "license": "MIT",
  "main": "./dist",
  "typings": "./dist",
  "scripts": {
    "add": "all-contributors add",
    "build": "npx gulp build",
    "build:demo": "cd demo && npm run build",
    "clean": "node bin/clean",
    "dev": "cd demo && npm start",
    "docs": "npx compodoc -p ./tsconfig.json -s",
    "preghpages": "cd demo && ng build --prod --base-href $BASE_HREF",
    "ghpages": "cd demo && npx angular-cli-ghpages --no-silent --repo=https://GH_TOKEN@github.com/$GH_ORG/$GH_REPO.git --name=$GH_NAME --email=$GH_EMAIL",
    "now-build": "npm run build:demo",
    "now-start": "npx serve demo/dist",
    "start": "npm run build",
    "test": "npx gulp test",
    "update": "ncu -a && cd demo && ncu -a",
    "update:contributors": "all-contributors generate",
    "watch": "npx gulp watch"
  },
  "dependencies": {
    "ng-snotify": "3.0.1",
    "sweetalert2": "6.9.0"
  },
