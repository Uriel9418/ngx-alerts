{
  "name": "@ngx-plus/ngx-alerts",
  "version": "0.5.3",
  "private": false,
  "description": "Advanced Angular Alerts",
  "keywords": [
    "alerts",
    "angular",
    "angular2",
    "angular4",
    "colmena",
    "ngx",
    "ngx-alerts",
    "ngx-plus"
  ],
  "homepage": "https://github.com/ngx-plus/ngx-alerts#readme",
  "bugs": {
    "url": "https://github.com/ngx-plus/ngx-alerts/issues"
  },
  "license": "MIT",
  "contributors": [
    "Brannon N. Darby II <brannon@ngx.plus> (https://github.com/brannon-darby)",
    "Bram Borggreve <borggreve@gmail.com> (https://github.com/beeman)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-plus/ngx-alerts.git"
  },
  "scripts": {
    "add:contributor": "all-contributors add",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "apps:affected": "./node_modules/.bin/nx affected apps",
    "build": "npm run build:libs && npm run build:demo",
    "build:affected": "./node_modules/.bin/nx affected build",
    "build:demo": "ng build --prod",
    "build:libs": "ng-packagr -p libs/ngx-alerts/package.json",
    "postbuild:libs": "cpx 'libs/ngx-alerts/src/scss/**/*.scss' dist/libs/ngx-alerts/scss",
    "clean": "rimraf node_modules",
    "precommit": "lint-staged",
    "e2e": "ng e2e",
    "e2e:affected": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "format:write": "./node_modules/.bin/nx format write",
    "preghpages": "ng build --prod --base-href $BASE_HREF",
    "ghpages": "npx angular-cli-ghpages --no-silent --repo=https://GH_TOKEN@github.com/$GH_ORG/$GH_REPO.git --name=$GH_NAME --email=$GH_EMAIL --dir=dist/apps/demo",
    "postinstall": "./node_modules/.bin/nx migrate check",
    "lint": "ng lint",
    "ng": "ng",
    "now-build": "npm run build:demo",
    "now-start": "npx serve demo/dist",
    "nx-migrate": "./node_modules/.bin/nx migrate",
    "nx-migrate:check": "./node_modules/.bin/nx migrate check",
    "nx-migrate:skip": "./node_modules/.bin/nx migrate skip",
    "publish": "npm publish dist/libs/ngx-alerts",
    "prerelease": "npm run build:libs",
    "release": "standard-version --message \"release: cut v%s\"",
    "postrelease": "node bin/prepublish.js && git push --follow-tags",
    "start": "ng serve",
    "test": "ng test",
    "update": "ncu -a"
  },
  "dependencies": {
    "lodash": "4.17.5",
    "ng-snotify": "4.2.0",
    "sweetalert2": "7.12.11"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.4.0",
    "@angular-devkit/schematics": "0.4.0",
    "@angular/animations": "5.2.5",
    "@angular/cli": "1.7",
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/compiler-cli": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/http": "5.2.5",
    "@angular/language-service": "5.2.5",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@angular/router": "5.2.5",
    "@angular/tsc-wrapped": "4.4.6",
    "@ngx-plus/ngx-forms": "0.5.3",
    "@ngx-plus/ngx-logos": "1.2.1",
    "@nrwl/nx": "0.7.4",
    "@nrwl/schematics": "0.7.4",
    "@schematics/angular": "0.4.0",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "4.14.104",
    "@types/node": "9.4.6",
    "bootstrap": "4.0.0",
    "codelyzer": "4.1.0",
    "core-js": "2.5.3",
    "cpx": "1.5.0",
    "font-awesome": "4.7.0",
    "husky": "0.14.3",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "lint-staged": "6.1.1",
    "ng-packagr": "2.0.0",
    "ngx-clipboard": "10.0.0",
    "prettier": "1.10.2",
    "prismjs": "1.11.0",
    "protractor": "5.3.0",
    "rxjs": "5.5.6",
    "standard-version": "4.3.0",
    "ts-node": "5.0.0",
    "tsickle": "0.27.2",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "zone.js": "0.8.20"
  },
  "peerDependencies": {
    "@angular/common": "5.2.5",
    "@angular/core": "5.2.5"
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "git add"
    ]
  }
}
